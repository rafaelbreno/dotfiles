{
  "codeLens.enable": true,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [ "go.mod", ".git/", "*.go" ],
      "filetypes": [ "go" ]
    },
    "ocaml-lsp": {
      "command": "opam",
      "args": ["config", "exec", "--", "ocamllsp"],
      "filetypes": ["ocaml", "reason"]
    }
  }
}
